{
    "name": "zi-chrome",
    "description": "Serve scripts from a prebuilt cache",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "dist/sw.js"
    },
    "action": {
        "default_popup": "/pages/popup.html",
        "default_icon": {
            "16": "/images/zi-16.png",
            "32": "/images/zi-32.png",
            "48": "/images/zi-48.png",
            "128": "/images/zi-128.png"
        }
    },
    "permissions": [
        "webRequest",
        "declarativeNetRequest",
        "declarativeNetRequestFeedback"
    ],
    "host_permissions": ["http://localhost/", "http://localhost:*/"]
}
