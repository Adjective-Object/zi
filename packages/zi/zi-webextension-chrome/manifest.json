{
    "name": "zi-chrome",
    "description": "Serve scripts from a prebuilt cache",
    "version": "1.0",
    "manifest_version": 2,
    "background": {
        "service_worker": "dist/sw.js"
    },
    "browser_action": {
        "default_popup": "/pages/popup.html",
        "default_icon": {
            "16": "/images/zi-16.png",
            "32": "/images/zi-32.png",
            "48": "/images/zi-48.png",
            "128": "/images/zi-128.png"
        }
    },
    "permissions": ["<all_urls>", "webRequest", "webRequestBlocking"]
}
